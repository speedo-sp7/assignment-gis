    "leaflet-routing-machine": "^3.2.12",
    "leaflet.markercluster": "^1.4.1",
    "mapbox-gl": "^0.51.0",
    "pg": "^6.4.2",
    "pg-format": "^1.0.4",
    "pg-parameterize": "^2.0.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-here": "0.0.1",
    "react-icons-kit": "^1.2.1",
    "react-leaflet": "^2.1.2",
